{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "*.gif",
      "use": "@vercel/static"
    },
    {
      "src": "*.ico",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "*.woff",
      "use": "@vercel/static"
    },
    {
      "src": "*.woff2",
      "use": "@vercel/static"
    },
    {
      "src": "*.ttf",
      "use": "@vercel/static"
    },
    {
      "src": "*.eot",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/detect",
      "destination": "/detect.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/profile",
      "destination": "/profile.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/create",
      "destination": "/create.html"
    },
    {
      "source": "/sms",
      "destination": "/sms.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/reset",
      "destination": "/reset.html"
    },
    {
      "source": "/interactive",
      "destination": "/interactive_main.html"
    },
    {
      "source": "/post",
      "destination": "/post.html"
    }
  ]
} 