{
  "version": 2,
  "name": "sms-shield",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "*.ttf",
      "use": "@vercel/static"
    },
    {
      "src": "*.woff2",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/detect",
      "destination": "/detect.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/profile",
      "destination": "/profile.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/logout",
      "destination": "/logout.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "FIREBASE_API_KEY": "AIzaSyBJqVavJpv6YaHvzJi49-fI2LsvTYGqijw",
    "FIREBASE_AUTH_DOMAIN": "smsphising.firebaseapp.com",
    "FIREBASE_PROJECT_ID": "smsphising",
    "FIREBASE_STORAGE_BUCKET": "smsphising.firebasestorage.app",
    "FIREBASE_MESSAGING_SENDER_ID": "92088698439",
    "FIREBASE_APP_ID": "1:92088698439:web:bae163a42c9996ee6e5362",
    "FIREBASE_MEASUREMENT_ID": "G-72619E1HYY",
    "GEMINI_API_KEY": "AIzaSyBioNiN-EnZCv1dYqSV83QDxV8RVK21Omg"
  }
} 